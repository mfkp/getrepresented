<% if current_user.is_admin? %>
<% title 'Admin Control Panel - Manage Users' %>
<ul>
<% @users.each do |user| %>
	<li><%= link_to user.username, :controller => 'users', :action => 'show', :id => user.id %></li>
<% end %>
</ul>
<% else %>
<% title 'Access Denied' %>
You do not have permission to view this page.
<% end %>